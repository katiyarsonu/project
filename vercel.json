{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "build" // Changed to "build" for Create React App
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html", // Changed to "/index.html" to serve the built React app
      "status": 200
    }
  ]
}
